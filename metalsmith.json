{
  "source": "./contents",
  "metadata": {
    "title": "My Wintersmith-Powered Blog",
    "description": "My second, super-cool, Wintersmith-powered blog."
  },
  "plugins": {
    "metalsmith-markdown": {},
    "metalsmith-permalinks": {
      "pattern": ":title"
    },
    "metalsmith-templates": {
      "engine": "jade",
      "directory": "./templates"
    },
    "metalsmith-sass": {
      "outputStyle": "expanded",
      "outputDir": "./contents/css/",
      "basePath": "./sass"
    }
  }
}